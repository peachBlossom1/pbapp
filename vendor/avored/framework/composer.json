{
  "name": "avored/framework",
  "description": "AvoRed Laravel E commerce Framework",
  "keywords": [
    "framework",
    "cart",
    "laravel",
    "e commerce",
    "laravel5",
    "shop",
    "shopping-cart",
    "e-commerce",
    "shopping cart",
    "e commerce"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Purvesh  ",
      "email": "ind.purvesh@gmail.com"
    }
  ],
  "type": "library",
  "require": {
    "php": ">=7.1.3",
    "symfony/yaml": "^4.0",
    "laravel/framework": "5.6.*"
  },
  "require-dev": {
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "AvoRed\\Framework\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AvoRed\\Framework\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AvoRed\\Framework\\Provider"
      ]
    }
  }
}
